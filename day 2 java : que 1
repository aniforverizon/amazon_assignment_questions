class Main {

  public int addNumbers(int a) {
    int b=0;
    
    if(a==200)
    {
    b=100;
    }
    
    else if (a==400)
    {b=200;
    }
    else if (a==500)
    {
    b= 370;
    }
    return b;
  }

  public static void main(String[] args) {
    
     Main r = new Main();
    int num1 = 200;
    int num2 = 2200;
int z = r.addNumbers(num1);

System.out.println("total cost after discount = "+ (num2-z));

   
  }
}
